# Nessus Configuration Template
# Copy this file to .env and update with your actual credentials

# Get your activation code from: https://www.tenable.com/products/nessus/nessus-essentials
NESSUS_ACTIVATION_CODE=YOUR_ACTIVATION_CODE_HERE

# Admin credentials for Nessus web interface
NESSUS_USERNAME=admin
NESSUS_PASSWORD=YourSecurePassword123

# Update settings
AUTO_UPDATE=all

# Optional linking key for Tenable.io (leave blank if not using)
LINKING_KEY=

# Scanner name (customize as needed)
SCANNER_NAME=homelab-scanner